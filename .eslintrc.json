{
    "env": {
        "browser": true,
      "es2020": true,
      "jest/globals": true
    },
  "plugins": ["jest"],
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": ["error", {"allow": ["warn", "error"]}]
    }
}
